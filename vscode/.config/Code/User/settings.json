{
  "update.mode": "none",
  "files.autoSave": "afterDelay",
  "debug.console.fontSize": 20,
  "terminal.integrated.fontSize": 18,
  "task.quickOpen.detail": false,
  "editor.fontFamily": "'Source Code Pro'",
  "editor.tabSize": 2,

  "workbench.colorCustomizations": {
    "editor.background": "#141418",
    "sideBar.background": "#17171f",
    "sideBarSectionHeader.background": "#121212",
    "statusBar.background": "#100c16",
    "scrollbarSlider.activeBackground": "#808080",
    "scrollbarSlider.hoverBackground": "#808080",
    "scrollbar.shadow": "#505050",
    "badge.background": "#808080",
    "terminal.background": "#080808",
    "terminal.foreground": "#80ff80",
    "panel.background": "#101016",
    "minimap.background": "#151518",
    "contrastBorder": "#303040"
  },
  "bazel.buildifierExecutable": "/usr/bin/buildifier",
  "bazel.buildifierFixOnFormat": true,
  "bazel-cpp-tools.compileCommands.targets": ["//main:hello-world"],
  "python.testing.pytestEnabled": true,
  "python.analysis.typeCheckingMode": "basic",
  "editor.inlineSuggest.enabled": true,
  "editor.detectIndentation": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [100],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "cmake.configureOnOpen": false,
  "dart.showInspectorNotificationsForWidgetErrors": false,

  "python.formatting.yapfArgs": ["--style", "{based_on_style: google, indent_width: 2}"],
  "python.formatting.provider": "yapf",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnType": true
  },

  "python.linting.pylintArgs": ["--indent-string=\"  \""],
  "editor.guides.indentation": false,

  "window.zoomLevel": 2,
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.enablePreview": false,
  "[cpp]": {
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true,
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "dart.lineLength": 120,
  "dart.renameFilesWithClasses": "prompt",
  "dart.flutterCreateAndroidLanguage": "java",
  "dart.flutterCustomEmulators": [],
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": true
  },
  "files.watcherExclude": {
    "p3": true,
    "python_env": true,
    "venv": true
  },
  "java.import.gradle.java.home": "/usr/lib/jvm/java-11-openjdk",
  "dart.flutterSdkPath": "/home/david/bin/flutter",
  "git.openRepositoryInParentFolders": "always",
  "editor.mouseWheelZoom": true,
  "files.exclude": {
    "**/__pycache*": true,
    "**/.build": true,
    "**/.pytest*": true
  },
  "mojo.modularHomePath": "/home/david/.modular",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cmake.showOptionsMovedNotification": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[cpp][dart]": {
    "editor.wordBasedSuggestions": "off"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "azureResourceGroups.selectedSubscriptions": [],
  "bicep.enableSurveys": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "debug.onTaskErrors": "showErrors",
  "npm.packageManager": "pnpm",
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "jupyter.interactiveWindow.creationMode": "perFile",
  "cloudcode.project": "wilder-419415",
  "html.format.wrapLineLength": 150,
  "prettier.printWidth": 200,
  "git.autofetch": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "telemetry.telemetryLevel": "off",
  "myvsext.enableSend": true,
  "todo-tree.highlights.defaultHighlight": {
    "background": "#422",
    "foreground": "#ffff00",
    "iconColour": "#ffff00",
    "fontWeight": "bold",
    "type": "text-and-comment"
  },
  "ruff.lineLength": 120,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "editor.colorDecoratorsActivatedOn": "hover",
  "editor.defaultColorDecorators": "auto",
  "testing.coverageToolbarEnabled": true,
  "window.titleBarStyle": "custom",
  "editor.formatOnSave": true,
  "cmake.pinnedCommands": ["workbench.action.tasks.configureTaskRunner", "workbench.action.tasks.runTask"],
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[cuda-cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "terminal.integrated.env.linux": {
    "TERM": "xterm-256color"
  },
  "workbench.colorTheme": "Tokyo Night",
  "geminicodeassist.project": "wilder-419415",
  "diffEditor.codeLens": true,
  "cloudcode.duetAI.project": "wilder-419415",
  "dotnet.formatting.organizeImportsOnFormat": true,
  "rubyLsp.formatter": "rubocop",
  "github.copilot.nextEditSuggestions.enabled": true,
  "claudeCode.preferredLocation": "panel",
  "claudeCode.allowDangerouslySkipPermissions": true,
  "claudeCode.initialPermissionMode": "bypassPermissions",
  "claudeCode.respectGitIgnore": false,
  "explorer.confirmPasteNative": false
}
