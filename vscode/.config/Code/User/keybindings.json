// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "f3",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "shift+f3",
    "command": "editor.action.peekDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+shift+f10",
    "command": "-editor.action.peekDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+shift+o",
    "command": "editor.action.organizeImports",
    "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)source\\.organizeImports\\b/"
  },
  {
    "key": "shift+alt+o",
    "command": "-editor.action.organizeImports",
    "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)source\\.organizeImports\\b/"
  },
  {
    "key": "ctrl+shift+-",
    "command": "-notebook.cell.split",
    "when": "editorTextFocus && inputFocus && notebookEditorFocused"
  },
  {
    "key": "ctrl+shift+-",
    "command": "-workbench.action.navigateForward",
    "when": "canNavigateForward"
  },
  {
    "key": "ctrl+shift+-",
    "command": "workbench.action.zoomOut"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "ctrl+alt+m",
    "command": "pylance.extractMethod"
  },
  {
    "key": "ctrl+shift+m",
    "command": "-rust-analyzer.matchingBrace",
    "when": "editorTextFocus && editorLangId == 'rust'"
  },
  {
    "key": "ctrl+shift+m",
    "command": "-workbench.actions.view.problems",
    "when": "workbench.panel.markers.view.active"
  },
  {
    "key": "ctrl+/",
    "command": "-emacs-mcx.isearchExit",
    "when": "editorFocus && findWidgetVisible && !isComposing"
  },
  {
    "key": "ctrl+/",
    "command": "-undo",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+v",
    "command": "-emacs-mcx.isearchExit",
    "when": "editorFocus && findWidgetVisible && !isComposing"
  },
  {
    "key": "ctrl+v",
    "command": "-emacs-mcx.scrollUpCommand",
    "when": "editorTextFocus && !suggestWidgetVisible"
  },
  {
    "key": "f1",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "ctrl+k f",
    "command": "-workbench.action.closeFolder",
    "when": "emptyWorkspaceSupport && workbenchState != 'empty'"
  },
  {
    "key": "ctrl+k f",
    "command": "workbench.explorer.fileView.focus"
  },
  {
    "key": "ctrl+shift+d",
    "command": "workbench.action.debug.selectandstart"
  },
  {
    "key": "ctrl+d",
    "command": "flutter.selectDevice"
  },
  {
    "key": "ctrl+shift+j",
    "command": "github.copilot.interactiveEditor.fix"
  },
  {
    "key": "ctrl+shift+k",
    "command": "workbench.action.chat.copilot.newChat"
  },
  {
    "key": "ctrl+shift+j",
    "command": "workbench.panel.chat.view.copilot.focus"
  },
  {
    "key": "ctrl+alt+j",
    "command": "workbench.action.decreaseViewSize"
  },
  {
    "key": "ctrl+alt+k",
    "command": "workbench.action.increaseViewSize"
  },
  {
    "key": "ctrl+alt+l",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+alt+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+alt+h",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+alt+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+t",
    "command": "testing.debugAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+; ctrl+c",
    "command": "-testing.debugAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+o",
    "command": "continue.optimizeCode"
  },
  {
    "key": "ctrl+alt+f",
    "command": "-list.find",
    "when": "listFocus && listSupportsFind"
  },
  {
    "key": "ctrl+alt+f",
    "command": "-repl.action.find",
    "when": "inDebugRepl || inDebugRepl && focusedView == 'workbench.panel.repl.view'"
  },
  {
    "key": "ctrl+alt+f",
    "command": "-list.find.replInputFocus",
    "when": "view == 'workbench.panel.repl.view'"
  },
  {
    "key": "ctrl+alt+f",
    "command": "continue.fixCode"
  },
  {
    "key": "ctrl+i",
    "command": "-continue.edit"
  },
  {
    "key": "ctrl+i",
    "command": "continue.edit"
  },

  {
    "command": "runCommands",
    "key": "ctrl+shift+5",
    "args": {
      "commands": [
        // show test explore
        "workbench.view.testing.focus",
        // run tests
        "testing.runAll",
        // hide the output panel which opens by default when running tests
        "workbench.action.togglePanel"
      ]
    }
  },
  {
    "key": "f4",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus"
  },
  {
    "key": "f8",
    "command": "-editor.action.marker.nextInFiles",
    "when": "editorFocus"
  },
  {
    "key": "f3",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "f3",
    "command": "-editor.action.nextMatchFindAction",
    "when": "editorFocus"
  },
  {
    "key": "f3",
    "command": "-list.find",
    "when": "listFocus && listSupportsFind"
  },
  {
    "key": "f3",
    "command": "-list.find.replInputFocus",
    "when": "view == 'workbench.panel.repl.view'"
  },
  {
    "key": "f3",
    "command": "-notebook.findNext.fromWidget",
    "when": "findWidgetVisible && notebookEditorFocused"
  }
]
